{
    "name": "Tab Sorter",
    "version": "0.2",
    "manifest_version": 2,
    "description": "Chrome addin to sort tabs",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Tab sorter",
        "default_popup": "background.html"
    },
    "permissions": [
        "tabs"
    ]
}

